package Package1;

public class Arithmetic_Class {

	public static void main(String[] args) {
		/*
		 * + Add, - sub, * mul, / div, % mod 
		 */
		
		int i1 = 88;
		int i2 = 22;
		
		int i3 = i1/i2;
		
		i3++;

		System.out.println(i3);

		i3--;
		
		System.out.println(i3);

		i3+=3;

		System.out.println(i3);

		i3*=2;

		System.out.println(i3);

		i3/=2;

		System.out.println(i3);

		i3+=7;
		i3%=2;

		System.out.println(i3);

	}
	

}
